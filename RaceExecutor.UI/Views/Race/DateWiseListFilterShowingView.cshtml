@model RaceExecutor.UI.Models.TrailRace
@{
    ViewBag.Title = "CategoryListView";
    Layout = null;
}


<h3 style="margin-top:50px; margin-bottom:20px; color:black;">Category Wise Ranked Data</h3>
<hr />

<table id="example" class="table table-striped table-bordered white-box" style="width:100%">
    <thead>
        <tr>
            <th>BIB Code</th>
            <th>Name</th>
            <th>Gender</th>
            <th>Age</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>Interval</th>
            <th>Distance</th>
            <th>Rank</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.DisplayList)
        {
            <tr>
                <td>@item.BIBCode</td>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.Age</td>
                <td>@item.StartTime.Value.ToLongTimeString()</td>
                <td>@item.EndTime.Value.ToLongTimeString()</td>
                <td>@item.Interval</td>
                <td>@item.Distance</td>
                <td>@item.Rank</td>
            </tr>
        }
    </tbody>
</table>
<link href="~/Content/datatable.bootstrap.min.css" rel="stylesheet" />
<script src="~/Content/datatable.min.js"></script>
<script>
    $(document).ready(function () {
        $('#example').DataTable();
    });
</script>

<script src="~/Content/datatable.min.js"></script>

